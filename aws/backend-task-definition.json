{
  "family": "hirenest-backend-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::431522527733:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "hirenest-backend",
      "image": "431522527733.dkr.ecr.us-east-1.amazonaws.com/hirenest-backend:latest",
      "portMappings": [
        {
          "containerPort": 5002,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        }
      ]
    }
  ]
} 